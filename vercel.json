{
  "buildCommand": "npm run build",
  "outputDirectory": ".",
  "cleanUrls": true,
  "trailingSlash": false,
  "alias": [
    "teramap.works",
    "www.teramap.works"
  ],
  "headers": [
    {
      "source": "/favicon.ico",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/favicon/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/images/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/",
      "destination": "/index.html"
    },
    {
      "source": "/kaydol",
      "destination": "/kaydol/index.html"
    },
    {
      "source": "/kaydol/(.*)",
      "destination": "/kaydol/index.html"
    },
    {
      "source": "/kaydol-custom",
      "destination": "/kaydol-custom.html"
    },
    {
      "source": "/giris",
      "destination": "/giris/index.html"
    },

    {
      "source": "/app",
      "destination": "/app/index.html"
    },
    {
      "source": "/app/",
      "destination": "/app/index.html"
    },
    {
      "source": "/app/messages",
      "destination": "/app/pages/messages.html"
    },
    {
      "source": "/app/venues",
      "destination": "/app/venues.html"
    },
    {
      "source": "/app/static/(.*)",
      "destination": "/app/static/$1"
    },
    {
      "source": "/admin",
      "destination": "/admin/index.html"
    }
  ]
} 