{
  "version": 2,
  "builds": [
    {
      "src": "index.html",
      "use": "@vercel/static"
    }
  ],
  "rewrites": [
    {
      "source": "/kaydol",
      "destination": "/kaydol/index.html"
    },
    {
      "source": "/kaydol/(.*)",
      "destination": "/kaydol/index.html"
    },
    {
      "source": "/kaydol-custom",
      "destination": "/kaydol-custom.html"
    },
    {
      "source": "/giris",
      "destination": "/giris/index.html"
    },
    {
      "source": "/hub",
      "destination": "/hub/index.html"
    },
    {
      "source": "/hub/",
      "destination": "/hub/index.html"
    },
    {
      "source": "/hub/giris",
      "destination": "/hub/giris.html"
    },
    {
      "source": "/hub/kaydol",
      "destination": "/hub/kaydol.html"
    },
    {
      "source": "/hub/mekanlar",
      "destination": "/hub/mekanlar.html"
    },
    {
      "source": "/hub/profil",
      "destination": "/hub/profil.html"
    },
    {
      "source": "/mekanlar",
      "destination": "/mekanlar.html"
    },
    {
      "source": "/genclik-ekipleri",
      "destination": "/genclik-ekipleri.html"
    },
    {
      "source": "/iletisim",
      "destination": "/iletisim.html"
    },
    {
      "source": "/ekibimiz",
      "destination": "/ekibimiz.html"
    }
  ],
  "headers": [
    {
      "source": "/favicon.ico",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/favicon/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/images/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
} 