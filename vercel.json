{
  "cleanUrls": true,
  "trailingSlash": false,
  "headers": [
    {
      "source": "/favicon.ico",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/favicon/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/images/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],

  "rewrites": [
    {
      "source": "/",
      "destination": "/index.html"
    },
    {
      "source": "/kaydol",
      "destination": "/kaydol/index.html"
    },
    {
      "source": "/kaydol/(.*)",
      "destination": "/kaydol/index.html"
    },
    {
      "source": "/kaydol-custom",
      "destination": "/kaydol-custom.html"
    },
        {
      "source": "/giris",
      "destination": "/giris/index.html"
    }
  ]
} 