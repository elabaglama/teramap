{
  "buildCommand": "npm run build",
  "outputDirectory": ".",
  "cleanUrls": true,
  "trailingSlash": false,
  "alias": [
    "teramap.works",
    "www.teramap.works"
  ],
  "headers": [
    {
      "source": "/favicon.ico",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/favicon/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/images/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],

    "rewrites": [
    {
      "source": "/",
      "destination": "/index.html"
    },
    {
      "source": "/kaydol",
      "destination": "/kaydol/index.html"
    },
    {
      "source": "/kaydol/(.*)",
      "destination": "/kaydol/index.html"
    },
    {
      "source": "/kaydol-custom",
      "destination": "/kaydol-custom.html"
    },
    {
      "source": "/giris",
      "destination": "/giris/index.html"
    },
    {
      "source": "/hub",
      "destination": "/hub/index.html"
    },
    {
      "source": "/hub/",
      "destination": "/hub/index.html"
    },
    {
      "source": "/hub/giris",
      "destination": "/hub/giris.html"
    },
    {
      "source": "/hub/kaydol",
      "destination": "/hub/kaydol.html"
    },
    {
      "source": "/hub/mekanlar",
      "destination": "/hub/mekanlar.html"
    },
    {
      "source": "/hub/profil",
      "destination": "/hub/profil.html"
    },
    {
      "source": "/hub/venue-detail",
      "destination": "/hub/venue-detail.html"
    },
    {
      "source": "/app",
      "destination": "/app/index.html"
    },
    {
      "source": "/app/",
      "destination": "/app/index.html"
    },
    {
      "source": "/app/venues",
      "destination": "/app/index.html"
    },
    {
      "source": "/app/messages",
      "destination": "/app/index.html"
    },
    {
      "source": "/app/notifications",
      "destination": "/app/index.html"
    }
  ]
} 