{
  "cleanUrls": true,
  "trailingSlash": false,
  "buildCommand": null,
  "headers": [
    {
      "source": "/favicon.ico",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/favicon/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/images/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],

    "rewrites": [
    {
      "source": "/",
      "destination": "/index.html"
    },
    {
      "source": "/kaydol",
      "destination": "/kaydol/index.html"
    },
    {
      "source": "/kaydol/(.*)",
      "destination": "/kaydol/index.html"
    },
    {
      "source": "/kaydol-custom",
      "destination": "/kaydol-custom.html"
    },
    {
      "source": "/giris",
      "destination": "/giris/index.html"
    },
    {
      "source": "/hub",
      "destination": "/hub/index.html"
    },
    {
      "source": "/hub/",
      "destination": "/hub/index.html"
    },
    {
      "source": "/hub/giris",
      "destination": "/hub/giris.html"
    },
    {
      "source": "/hub/kaydol",
      "destination": "/hub/kaydol.html"
    },
    {
      "source": "/hub/mekanlar",
      "destination": "/hub/mekanlar.html"
    },
    {
      "source": "/hub/profil",
      "destination": "/hub/profil.html"
    }
  ]
} 