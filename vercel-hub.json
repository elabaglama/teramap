{
  "version": 2,
  "name": "teramap-hub",
  "alias": ["teramap.hub", "hub.teramap.vercel.app"],
  "cleanUrls": true,
  "trailingSlash": false,
  "public": false,
  "github": {
    "silent": true
  },
  "headers": [
    {
      "source": "/favicon.ico",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/favicon/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/images/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/",
      "destination": "/hub/index.html"
    },
    {
      "source": "/giris",
      "destination": "/hub/giris.html"
    },
    {
      "source": "/kaydol",
      "destination": "/hub/kaydol.html"
    },
    {
      "source": "/mekanlar",
      "destination": "/hub/mekanlar.html"
    },
    {
      "source": "/profil",
      "destination": "/hub/profil.html"
    }
  ]
} 